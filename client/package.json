{
  "name": "forecast",
  "version": "1.0.0",
  "description": "---",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=production npx webpack-dev-server --content-base ./target --no-inline --config ./util/build/webpack.config.babel.js",
    "start:dev": "NODE_ENV=development npx webpack-dev-server --content-base ./target --no-inline --config ./util/build/webpack.config.babel.js",
    "build": "NODE_ENV=production npm run build-client:webpack",
    "build:dev": "NODE_ENV=development npm run build-client:webpack",
    "build-client:webpack": "npx babel-node ./util/build/build.js"
  },
  "babel": {
    "minified": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "@babel/preset-stage-0",
        {
          "decoratorsLegacy": true
        }
      ]
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/node": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-stage-0": "^7.0.0-beta.51",
    "@types/react-bootstrap": "^0.32.9",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.2.7",
    "awesome-typescript-loader": "^5.0.0",
    "babel-loader": "^8.0.0-beta.3",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^4.0.0-alpha",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "react": "^16.0.6",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.0.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.3.2"
  }
}
